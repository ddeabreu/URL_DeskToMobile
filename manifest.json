{
"name" :"extension chrome",
"version" :"1.0",
"manifest_version": 2,
"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
"description" :"QR URL",
"options_page":"page_options.html",
"browser_action" :
	{
		 "default_icon": {                    // optional
            "16": "images/icone16.png",           // optional
            "24": "images/icone24.png",           // optional
            "32": "images/icone32.png",
            "48": "images/icone48.png",
            "128": "images/icone128.png"            // optional
          },
        "default_title": "QR URL",      // optional; shown in tooltip
		"default_popup" :"qr_url.html"
	},
"permissions": [
"activeTab",
"https://ajax.googleapis.com/",
"https://api.qrserver.com/"
  ]
}
